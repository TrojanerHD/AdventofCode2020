  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Deno",
      "request": "launch",
      "type": "pwa-node",
      "program": "src/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect-brk", "--unstable", "--allow-read"],
      "attachSimplePort": 9229,
      "outputCapture": "std",
      "args": ["8"]
    }
  ]
}
